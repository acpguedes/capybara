{
  "manifest_version": 3,
  "name": "Capybara",
  "version": "0.0.1",
  "description": "Unified bookmarks across browsers with categorization and fast search.",
  "action": {
    "default_popup": "dist/popup/index.html"
  },
  "background": {
    "service_worker": "dist/background/index.js"
  },
  "permissions": [
    "bookmarks",
    "storage"
  ],
  "host_permissions": [
    "https://api.openai.com/*"
  ]
}
